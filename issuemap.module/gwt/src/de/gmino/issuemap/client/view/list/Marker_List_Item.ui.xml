<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
		.parent{
			float: left;
			width: 135px;
			height: 130px;
			border: 1px solid #ccc;
		}
		.markerImage{
			padding-top:10px;
			padding-left:20px;
			padding-right:20px;
			cursor: pointer;
		}
		.markerLabel{
			cursor: pointer;
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:VerticalPanel  styleName="{style.parent}" ui:field="parent">
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:Image ui:field="markerImage" styleName="{style.markerImage}"></g:Image></g:cell>
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:Label ui:field="markerNameLabel" styleName="{style.markerLabel}"></g:Label></g:cell>
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:CheckBox ui:field="markerCheckbox"></g:CheckBox></g:cell>
	    </g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 