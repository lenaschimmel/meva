<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
		.parent{
			float: left;
			width: 150px;
			height: 120px;
			border: 1px solid #ccc;
			padding-top: 10px;
			}
		
	</ui:style>
	<g:HTMLPanel>
	    <g:VerticalPanel  styleName="{style.parent}" ui:field="parent">
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:Image ui:field="markerImage"></g:Image></g:cell>
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:Label ui:field="markerNameLabel"></g:Label></g:cell>
	    	<g:cell horizontalAlignment="ALIGN_CENTER"><g:CheckBox ui:field="markerCheckbox"></g:CheckBox></g:cell>
	    </g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 