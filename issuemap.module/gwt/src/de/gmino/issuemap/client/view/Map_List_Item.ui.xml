<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.parent{
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel styleName="{style.parent}">
			<g:Label width="50px" ui:field="id"></g:Label>
			<g:Label width="200px" ui:field="subdomain"></g:Label>
			<g:Label width="300px" ui:field="title"></g:Label>
			<g:Button ui:field="edit">Bearbeiten</g:Button>
			<g:Button ui:field="copy">Kopieren</g:Button>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 