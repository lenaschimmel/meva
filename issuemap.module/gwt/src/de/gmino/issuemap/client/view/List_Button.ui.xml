<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
    <ui:with field="resources"
		type="de.gmino.issuemap.client.resources.ImageResources" />
    
    
	<ui:style>
		.button {
		position: absolute;
		right: 0px;
		top: 45%;
		cursor: pointer;
		}

	</ui:style>
	<g:HTMLPanel>
		<g:PushButton styleName="{style.button}" ui:field="list_button"
			width="40px" title="Liste der wichtigsten Einträge öffnen">

			<g:upFace image='{resources.list1}' />
			<g:downFace image='{resources.list1}' />
			<g:upHoveringFace image='{resources.list_hover}' />
			<g:downHoveringFace image='{resources.list1}' />
		</g:PushButton>
	</g:HTMLPanel>
</ui:UiBinder> 