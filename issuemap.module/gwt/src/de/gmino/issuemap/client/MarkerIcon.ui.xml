<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.icon{
			position: absolute;
			top: 0px;
			z-index: -100;
}
	</ui:style>

	<g:HTMLPanel><g:Image styleName="{style.icon}" ui:field="icon"></g:Image></g:HTMLPanel>
	

</ui:UiBinder> 